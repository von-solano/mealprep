<!DOCTYPE html>
<html>
  <head>
    <title>Dashboard</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <%- include('partial/navbar') %> <!-- including navbar -->
    <h1>Dashboard</h1>
    
    <!-- macro overview section -->
    <section id="macros">
      <div class="macro-box">
        <h3>Calories</h3>
      </div>

      <div class="macro-box">
        <h3>Protein</h3>
      </div>

      <div class="macro-box">
        <h3>Carbs</h3>
      </div>

      <div class="macro-box">
        <h3>Fat</h3>
      </div>
    </section>

    <section id="recents">
      <div class="recent">
        <h3>Today's Plan</h3>
      </div>
      <div class="recent">
        <h3>Recent Recipes</h3>
      </div>
    </section>

    <section id="progress">
      <h3>Progress</h3>
    </section>

  </body>
</html>
